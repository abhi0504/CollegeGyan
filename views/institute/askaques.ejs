<%- include('../partials/header') %>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>

<form class="form-horizontal" role="form">
    <div class="form-group">
        <label for="to" class="col-sm-1 control-label">Topic</label>
        <div class="col-sm-11">
              <input type="email" class="form-control select2-offscreen" id="to" placeholder="Enter the topic of your ques" tabindex="-1">
        </div>
      </div>
    <div class="form-group">
        <label for="cc" class="col-sm-1 control-label">Add Labels</label>
        <div class="col-sm-11">
              <input type="email" class="form-control select2-offscreen" id="cc" placeholder="Add some cool labels here" tabindex="-1">
        </div>
      </div>
  
</form>
<div class="col-sm-11 col-sm-offset-1">

    <br>	
					
    <div class="form-group">
        <textarea class="form-control" id="message" name="body" rows="12" placeholder="Explain your Question..."></textarea>
    </div>
    
    <div class="form-group">	
        <button type="submit" class="btn btn-success">Send</button>
        <button type="submit" class="btn btn-danger">Discard</button>
    </div>

<%- include('../partials/footer') %>
