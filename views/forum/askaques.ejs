<%- include('../partials/header') %>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>

<form class="form-horizontal" role="form" action="/AskAQues" method="POST">
    <div class="form-group">
        <label class="col-sm-1 control-label">Topic</label>
        <div class="col-sm-11">
              <input  class="form-control select2-offscreen" id="to" name="topic" placeholder="Enter the topic of your ques" tabindex="-1">
        </div>
      </div>
    <div class="form-group">
        <label  class="col-sm-1 control-label">Add Labels</label>
        <div class="col-sm-11">
              <input  class="form-control select2-offscreen" id="cc" name="labels" placeholder="Add some cool labels here" tabindex="-1">
        </div>
      </div>

      <br>	
					
    <div class="form-group">
        <textarea class="form-control" id="message" name="body" rows="12" placeholder="Explain your Question..."></textarea>
    </div>
    
    <div class="form-group">	
        <button type="submit" class="btn btn-success">Submit</button>
        <button type="submit" class="btn btn-danger">Discard</button>
    </div>
  
</form>
<div class="col-sm-11 col-sm-offset-1">

    

<%- include('../partials/footer') %>
